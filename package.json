{
  "name": "u2d",
  "version": "1.0.0",
  "bin": "./bin/cli.js",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "exports": {
    "import": "./dist/main.mjs",
    "require": "./dist/main.js"
  },
  "description": "Configurable Version Management",
  "homepage": "https://github.com/godaddy/u2d#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godaddy/u2d.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "version",
    "semver",
    "check",
    "up 2 date",
    "up to date",
    "utd"
  ],
  "scripts": {
    "tsup": "tsup ./src/main.ts ./src/cli.ts --format esm,cjs --clean",
    "build": "npm run tsup -- --dts",
    "watch": "npm run tsup -- --watch",
    "types": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.ts \"./src/**\" \"./test/**\"",
    "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "unit": "npm run jest -- ./test/unit",
    "integration": "npm run jest -- ./test/integration",
    "coverage": "npm run unit -- --coverage",
    "test": "npm-run-all lint --parallel types coverage",
    "prepublishOnly": "npm-run-all --aggregate-output lint --parallel types unit build --serial integration"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "cosmiconfig": "^7.0.1",
    "execa": "^5.1.1",
    "listr2": "^4.0.5",
    "node-fetch": "^2.6.7",
    "semver": "^7.3.7",
    "strip-ansi": "^6.0.1",
    "supports-esm": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.25",
    "@types/node-fetch": "^2.6.1",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.6",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^28.0.3",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^28.0.0",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts-esm",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./src/**"
    ],
    "globals": {
      "extensionsToTreatAsEsm": [
        ".ts",
        ".js"
      ],
      "ts-jest": {
        "useESM": true,
        "tsconfig": {
          "allowJs": true,
          "importHelpers": true,
          "esModuleInterop": true
        }
      }
    }
  },
  "u2d": {
    "local": true,
    "engines": {
      "node": ">=16",
      "npm": ">=8"
    }
  },
  "license": "MIT"
}
